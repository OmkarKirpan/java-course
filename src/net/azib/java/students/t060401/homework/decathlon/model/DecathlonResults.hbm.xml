<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.azib.java.students.t060401.homework.decathlon.model.ResultsVO" table="results">
        <id name="id" column="id"/>
        <property name="athleteId" column="athlete_id"/>
        <property name="competitionId" column="competition_id"/>
        <property name="sprint100m" column="race_100m"/>
        <property name="longJump" column="long_jump"/>
        <property name="shotPut" column="shot_put"/>
        <property name="highJump" column="high_jump"/>
        <property name="sprint400m" column="race_400m"/>
        <property name="hurdles110m" column="hurdles_110m"/>
        <property name="discusThrow" column="discus_throw"/>
        <property name="poleVault" column="pole_vault"/>
        <property name="javelinThrow" column="javelin_throw"/>
        <property name="race1500m" column="race_1500m"/>
    </class>
</hibernate-mapping>